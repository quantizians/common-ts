{
  "name": "@quantiz/common",
  "version": "0.0.1",
  "description": "Common utility types and function for TS/JS usage",
  "main": "lib/index.js",
  "types": "lib/types/index.d.ts",
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.map"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint:es": "eslint src --ext .ts,.tsx",
    "prettier": "prettier --check 'src/*.{ts,js,json}'",
    "prebuild": "run-p lint:es prettier",
    "build": "rimraf lib && ts-patch install -s && tsc --project tsconfig.build.json",
    "test": "jest",
    "test:ci": "jest --ci --json --coverage --testLocationInResults --outputFile=coverage/report.json",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quantizians/common-ts.git"
  },
  "keywords": [
    "typescript",
    "javascript",
    "types",
    "typing",
    "utility",
    "utils"
  ],
  "author": "Quang Phan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/quantizians/common-ts/issues"
  },
  "homepage": "https://github.com/quantizians/common-ts",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "eslint": "^8.10.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-modules-newline": "^0.0.6",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "ts-patch": "^2.0.1",
    "ttypescript": "^1.5.13",
    "typescript": "^4.6.2",
    "typescript-transform-paths": "^3.3.1"
  },
  "engines": {
    "pnpm": ">= 6.32.2",
    "node": ">=14.0.0"
  }
}
